{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "cs/accordion",
  "version": "0.1.0",
  "title": "CS Accordion Block",
  "category": "design",
  "icon": "menu",
  "description": "Shows content in an accordion.",
  "supports": {
	"html": false,
	"align": true,
	"anchor": true
  },
  "attributes": {
	"duration": {
	  "type": "number",
	  "default": 600
	},
	"collapse": {
	  "type": "boolean",
	  "default": false
	},
	"showMultiple": {
	  "type": "boolean",
	  "default": false
	}
  },
  "textdomain": "cs-accordion",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "script": "file:./frontend.js"
}
